#include<iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	 long long int a,b;
	    cin>>a>>b;
	 long long int n=a;
	 long long int m=b;
	    while(a!=b){
	        if(a>b){
	            a=a-b;
	        }
	        else{
	        b=b-a;
	    }
	}
	
	cout<<(n*m)/a<<' '<<a<<' '<<endl;
	}
	
	return 0;
}
