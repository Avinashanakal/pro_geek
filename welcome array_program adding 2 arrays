#include <iostream>

using namespace std;

int main(){
    int n;
    cin>>n;
   unsigned long long int a[100000],b[100000],c[100000];
    for(int i=0;i<n;i++){
        cin>>a[i];
        cout<<' ';
    }
    for(int i=0;i<n;i++){
        cin>>b[i];
        cout<<' ';
    }
    for(int i=0;i<n;i++){
        c[i]=a[i]+b[i];
        cout<<c[i]<<' ';
    }
    return 0;
}
